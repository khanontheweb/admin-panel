<p id="notice"><%= notice %></p>

<h1>Test Scaffolds</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_scaffolds.each do |test_scaffold| %>
      <tr>
        <td><%= test_scaffold.email %></td>
        <td><%= link_to 'Show', test_scaffold %></td>
        <td><%= link_to 'Edit', edit_test_scaffold_path(test_scaffold) %></td>
        <td><%= link_to 'Destroy', test_scaffold, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test Scaffold', new_test_scaffold_path %>
